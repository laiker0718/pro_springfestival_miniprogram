}
.download-tip {
  color: white;
  font-size: 32rpx;
  display: flex;
  position: absolute;
  justify-content: center;
  align-items: center;
  text-align: center;
  width: 50%;
  height: 115rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
